<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextField fx:id="login" layoutX="490.0" layoutY="320.0" prefHeight="25.0" prefWidth="300.0" promptText="Login" />
        <PasswordField fx:id="passwordField" layoutX="490.0" layoutY="360.0" onAction="#submit" prefHeight="26.0" prefWidth="300.0" promptText="Password" />
      <TextField fx:id="passwordTextField" layoutX="490.0" layoutY="361.0" onAction="#submit" prefHeight="25.0" prefWidth="300.0" promptText="Password" visible="false" />
        <CheckBox fx:id="showPasswordCheckBox" layoutX="490.0" layoutY="394.0" mnemonicParsing="false" onAction="#showPassword" text="Show password" />
        <Button layoutX="600.0" layoutY="400.0" mnemonicParsing="false" onAction="#submit" prefWidth="80.0" text="Login" />
        <Button layoutX="600.0" layoutY="431.0" mnemonicParsing="false" onAction="#signIn" prefWidth="80.0" text="Sign In" />
      <ImageView fx:id="avatarImageView" fitHeight="162.0" fitWidth="200.0" layoutX="559.0" layoutY="128.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../no-avatar-png-8.png" />
         </image>
      </ImageView>

    </children>
</AnchorPane>
