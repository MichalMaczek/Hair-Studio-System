<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="546.0" prefWidth="851.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="visits" layoutX="48.0" layoutY="61.0" prefHeight="423.0" prefWidth="755.0">
         <columns>
            <TableColumn fx:id="employeeVisitTableColumn" prefWidth="150.0" text="Employee" />
            <TableColumn fx:id="statusOfVisitTableColumn" prefWidth="150.0" text="Status" />
            <TableColumn fx:id="startTimeOfVisitTableColumn" prefWidth="150.0" text="Start Time" />
            <TableColumn fx:id="endTimeOfVisitTableColumn" prefWidth="150.0" text="End Time" />
            <TableColumn fx:id="typeOfServiceTableColumn" prefWidth="150.0" text="Service" />
         </columns>
      </TableView>
      <TextField fx:id="filterVisits" layoutX="47.0" layoutY="26.0" prefHeight="26.0" prefWidth="755.0" promptText="Search visits' details" />
      <Button fx:id="returnToHomePageButton" layoutX="48.0" layoutY="495.0" mnemonicParsing="false" onAction="#returnToHomePage" text="Return To Home Page" />
      <Button fx:id="confirmVisitButton" layoutX="631.0" layoutY="495.0" mnemonicParsing="false" onAction="#confirmVisit" text="Confirm Visit" />
      <Button fx:id="deleteVisitButton" layoutX="727.0" layoutY="495.0" mnemonicParsing="false" onAction="#deleteVisit" text="Delete Visit" />
   </children>
</AnchorPane>
