<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="446.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="filterEmployee" layoutX="10.0" layoutY="14.0" prefHeight="26.0" prefWidth="604.0" promptText="Search employees information" />
      <Button fx:id="returnToHomePageButton" layoutX="10.0" layoutY="406.0" mnemonicParsing="false" onAction="#returnToHomePage" text="Return To Home Page" />
      <TableView fx:id="employeeInformationTableView" layoutX="10.0" layoutY="52.0" prefHeight="342.0" prefWidth="603.0">
        <columns>
          <TableColumn fx:id="employeeFirstNameTableColumn" prefWidth="150.0" text="First Name" />
          <TableColumn fx:id="employeeLastNameTableColumn" prefWidth="150.0" text="Last Name" />
            <TableColumn fx:id="employeePhoneNumberTableColumn" prefWidth="150.0" text="Phone Number" />
            <TableColumn fx:id="employeeEmailAddressTableColumn" prefWidth="150.0" text="E-Mail Address" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
